n.scrollIntoView({ behavior: \"smooth\" });\n  };\n  const changeScroll = () =>\n    document.body.scrollTop > 80 || document.documentElement.scrollTop > 80\n      ? setScroll(true)\n      : setScroll(false);\n\n  window.addEventListener(\"scroll\", changeScroll);\n\n  const TbLetterComponents = [];\n\n  for (let i = 0; i < profile.logo.length; i++) {\n    const letter = profile.logo[i];\n    const component = TbIcons[`TbLetter${letter}`];\n    TbLetterComponents.push(component);\n  }\n\n  return (\n    <>\n      <Flex\n        bg={useColorModeValue(\"gray.100\", \"gray.900\")}\n        px={4}\n        h={16}\n        boxShadow={scroll ? \"base\" : \"none\"}\n        zIndex=\"sticky\"\n        position=\"fixed\"\n        as=\"header\"\n        alignItems={\"center\"}\n        justifyContent={\"space-between\"}\n        w=\"100%\"\n      >\n        <Link onClick={scrollToHero}>\n          <HStack>\n            {TbLetterComponents.map((Component, index) => (\n              <Component key={index} color={colors[color]} />\n            ))}\n          </HStack>\n        </Link>\n\n        <Flex alignItems={\"center\"}>\n          <Stack direction={\"row\"} spacing={7}>\n            {isLargerThanMD ? (\n              <>\n                <Button variant=\"ghost\" onClick={scrollToAbout}>\n                  About\n                </Button>\n                <Button variant=\"ghost\" onClick={scrollToProjects}>\n                  Projects\n                </Button>\n                <Button variant=\"ghost\" onClick={scrollToContact}>\n                  Contact\n                </Button>\n              </>\n            ) : (\n              <></>\n            )}\n            <Button onClick={toggleColorMode}>\n              {colorMode === \"light\" ? <MoonIcon /> : <SunIcon />}\n            </Button>\n\n            {isLargerThanMD ? (\n              <></>\n            ) : (\n              <>\n                <Button\n                  as={IconButton}\n                  icon={<Ha